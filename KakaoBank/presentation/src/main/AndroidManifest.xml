<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.hs.dgsw.kakaobank">

    <uses-permission android:name="android.permission.INTERNET" /> <!-- 갤러리 권한 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".widget.KakaoBankApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity
            android:name=".view.activity.OpenUpSuccessActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.terms.InforAccountAcitivty"
            android:exported="true" />
        <activity
            android:name=".view.activity.OpenupActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.OpenUpAccountActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.terms.BorrowAccountActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.EasyLoginActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.WelcomeActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.SignupActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>